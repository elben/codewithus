<!DOCTYPE html>
<html>
<head>
  <title>Code With Us</title>
  <%= stylesheet_link_tag :all %>
  <!--<%= javascript_include_tag :defaults %>-->
  <%= javascript_include_tag 'jquery-1.4.1.js' %>
  <%= javascript_include_tag 'jquery.watermark.js' %>
  <%= javascript_include_tag 'rails' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tag %>
  <link href='http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz&subset=latin' rel='stylesheet' type='text/css'>
</head>
<body>
  <div id="flash">
    <% flash.each do |key, value| %>
      <div class="flash <%= h key %>"><%= h value %></div>
    <% end %> 
  </div>

  <div class="main">
  <%= yield %>
  </div>

</body>
</html>
